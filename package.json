{
  "name": "esn-sections-website",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "start:local-network": "ng serve -o --host 0.0.0.0 --disable-host-check",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod",
    "dev:ssr": "ng run esn-sections-website:serve-ssr",
    "serve:ssr": "node dist/esn-sections-website/server/main.js",
    "build:ssr": "ng build && ng run esn-sections-website:server",
    "prerender": "ng run esn-sections-website:prerender",
    "prerender:freiburg": "ng run esn-sections-website:prerender --configuration freiburg"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.1.1",
    "@angular/common": "13.1.1",
    "@angular/compiler": "13.1.1",
    "@angular/core": "13.1.1",
    "@angular/forms": "13.1.1",
    "@angular/platform-browser": "13.1.1",
    "@angular/platform-browser-dynamic": "13.1.1",
    "@angular/platform-server": "13.1.1",
    "@angular/router": "13.1.1",
    "@fullcalendar/angular": "^5.9.0",
    "@fullcalendar/daygrid": "^5.9.0",
    "@fullcalendar/interaction": "^5.9.0",
    "@ngneat/tailwind": "^7.0.3",
    "@nguniversal/builders": "^13.0.0",
    "@nguniversal/express-engine": "12.1.0",
    "autoprefixer": "^10.4.0",
    "express": "^4.15.2",
    "ng-gallery": "^5.0.0",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "yarn-upgrade-all": "0.5.4",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.1",
    "@angular/cli": "13.1.1",
    "@angular/compiler-cli": "13.1.1",
    "@tailwindcss/typography": "^0.5.0",
    "@types/express": "4.17.13",
    "@types/jasmine": "3.9.1",
    "@types/node": "15.12.2",
    "husky": "7.0.2",
    "jasmine-core": "3.9.0",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "lint-staged": "11.1.2",
    "ngx-markdown": "12.0.1",
    "postcss": "^8.4.5",
    "prettier": "2.4.1",
    "protractor": "7.0.0",
    "tailwindcss": "^3.0.7",
    "ts-node": "10.2.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,scss,html,css,md}": "prettier --write"
  }
}
